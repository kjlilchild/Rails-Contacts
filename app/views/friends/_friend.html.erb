
<div id="<%= dom_id friend %>">
  <table>
    <p>
      <strong>First name:</strong>
      <%= friend.first_name %>
    </p>
    <p>
      <strong>Last name:</strong>
      <%= friend.last_name %>
    </p>
    <p>
      <strong>Email:</strong>
      <%= friend.email %>
    </p>
    <p>
      <strong>Phone:</strong>
      <%= friend.phone %>
    </p>
      <strong>Twitter:</strong>
      <%= friend.twitter %>
    </p>
  </table>
</div>

